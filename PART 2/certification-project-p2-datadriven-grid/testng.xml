<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">

	<groups>
		<run>
			<include name="Grid"></include>
		</run>
	</groups>

	<test thread-count="5" name="Test">
		<parameter name="browser" value="chrome"></parameter>
		<parameter name="grid" value="false"></parameter>
		<parameter name="url" value="https://www.shopclues.com/"></parameter>
		<parameter name="searchTermNike" value="Nike"></parameter>
		<parameter name="searchTermSamsung" value="Samsung Mobile"></parameter>
		<parameter name="searchTermApple" value="Apple Phone"></parameter>
		<parameter name="searchTermHeadphone" value="headphone"></parameter>
		<classes>
			<class name="test.certProjPart21" />
			<class name="test.certProjPart22" />
			<class name="test.certProjPart23" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
